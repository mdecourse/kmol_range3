<t tx="leo.20201108101536.122">void RModel::createStreamLine(RStreamLine &amp;rStreamLine) const
{
    rStreamLine.clear();

    uint variablePosition = this-&gt;findVariable(rStreamLine.getVariableType());
    if (variablePosition == RConstants::eod)
    {
