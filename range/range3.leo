                 type == MODEL_TREE_VOLUME ||
                 type == MODEL_TREE_VECTOR_FIELD ||
                 type == MODEL_TREE_SCALAR_FIELD ||
                 type == MODEL_TREE_STREAM_LINE ||
                 type == MODEL_TREE_CUT ||
