    QString ext = RFileManager::getExtension(fileName);

    try
    {
        if (ext == RMaterial::getDefaultFileExtension(false))
        {
            this-&gt;readAscii(fileName);
        }
        else if (ext == RMaterial::getDefaultFileExtension(true))
        {
            this-&gt;readBinary(fileName);
