        emit this-&gt;scaleChanged(this-&gt;scaleCenter-&gt;getPosition(),
                                RR3Vector(this-&gt;xScaleLineEdit-&gt;getValue(),
                                          this-&gt;yScaleLineEdit-&gt;getValue(),
                                          this-&gt;zScaleLineEdit-&gt;getValue()));
