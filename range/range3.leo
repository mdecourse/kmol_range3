        removed = true;
    }
    if (decrement)
    {
        for (uint i=0;i&lt;this-&gt;elementGroupIDs.size();i++)
        {
            if (this-&gt;elementGroupIDs[i] &gt; elementGroupID)
            {
                this-&gt;elementGroupIDs[i]--;
            }
        }
    }
    return removed;
}

