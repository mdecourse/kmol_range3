<t tx="leo.20201108101534.169">void RFileIO::writeAscii(RSaveFile &amp;outFile, const RVariableApplyType &amp;variableApplyType, bool addNewLine)
{
    if (!addNewLine)
    {
        outFile.getTextStream() &lt;&lt; int(variableApplyType);
    }
    else
    {
