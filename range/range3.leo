{
    { Qt::NoModifier,                          0, Qt::NoButton,                     Qt::NoScrollPhase, GL_ACTION_EVENT_NONE              },
    { Qt::NoModifier,                          0, Qt::LeftButton,                   Qt::NoScrollPhase, GL_ACTION_EVENT_TRANSLATE         },
