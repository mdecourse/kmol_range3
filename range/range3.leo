#include &lt;QVBoxLayout&gt;
#include &lt;QHBoxLayout&gt;
#include &lt;QIcon&gt;
#include &lt;QPushButton&gt;
#include &lt;QLabel&gt;

#include "bool_difference_dialog.h"
#include "model_action.h"
