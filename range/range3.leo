    { Qt::NoModifier,                          0, Qt::LeftButton | Qt::RightButton, Qt::NoScrollPhase, GL_ACTION_EVENT_ROTATE            },
    { Qt::NoModifier,                          0, Qt::RightButton,                  Qt::NoScrollPhase, GL_ACTION_EVENT_ZOOM              },
