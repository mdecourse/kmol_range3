    RLogger::trace("void Session::setModel(uint position, const Model &amp;model)\n");
    bool locked = this-&gt;trylock();
    this-&gt;models[int(position)] = model;
    if (locked)
    {
        this-&gt;unlock();
    }
    this-&gt;setModelChanged(position);
}

