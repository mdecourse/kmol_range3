                          this-&gt;elementRadiativeHeat,
                          0.0);
    this-&gt;recoverVariable(R_VARIABLE_JOULE_HEAT,
                          R_VARIABLE_APPLY_ELEMENT,
                          this-&gt;pModel-&gt;getNElements(),
                          0,
                          this-&gt;elementJouleHeat,
                          0.0);
}

