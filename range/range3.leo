        else
        {
            throw RError(R_ERROR_INVALID_FILE_NAME,R_ERROR_REF, "Unknown extension \"" + ext + "\".");
        }
    }
    catch (RError &amp;error)
    {
        throw error;
    }
    catch (const std::exception &amp;error)
    {
        throw RError(R_ERROR_APPLICATION,R_ERROR_REF, "%s", error.what());
    }
    catch (...)
