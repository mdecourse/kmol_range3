<t tx="leo.20201108101518.109">void DocumentTree::populate(void)
{
    this-&gt;blockSignals(true);
    this-&gt;clear();
    this-&gt;blockSignals(false);

    for (uint i=0;i&lt;Session::getInstance().getNModels();i++)
