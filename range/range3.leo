<t tx="leo.20201108101543.77">bool RSolverRadiativeHeat::hasConverged(void) const
{
    if (this-&gt;oldPatchHeatNorm &lt; RConstants::eps)
    {
        return (this-&gt;patchHeatNorm &lt; RConstants::eps);
