    R_ERROR_ASSERT (valpos &lt; this-&gt;getNValues ());

    RRVector valueVector(this-&gt;getNVectors());

    for (unsigned int i=0;i&lt;this-&gt;getNVectors();i++)
    {
        valueVector[i] = this-&gt;getValue(i,valpos);
    }

    return valueVector;
} /* RVariable::getValueVector */


