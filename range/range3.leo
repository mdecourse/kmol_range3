<t tx="leo.20201108101539.60">static bool CircumCircle(double  xp, double  yp,
                         double  x1, double  y1,
                         double  x2, double  y2,
                         double  x3, double  y3,
                         double *xc, double *yc, double *r)
{
    double m1,m2,mx1,mx2,my1,my2;
    double dx,dy,rsqr,drsqr;
    double tolerance = 0.0;

    tolerance = RConstants::eps;
    tolerance = 0.0;

   /* Check for coincident points */
    if (std::fabs(y1-y2) &lt;= tolerance &amp;&amp;
        std::fabs(y2-y3) &lt;= tolerance)
    {
        return (0);
    }

    if (std::fabs(y2-y1) &lt;= tolerance)
    {
        m2 = - (x3-x2) / (y3-y2);
        mx2 = (x2 + x3) / 2.0;
        my2 = (y2 + y3) / 2.0;
        *xc = (x2 + x1) / 2.0;
        *yc = m2 * (*xc - mx2) + my2;
    }
    else if (std::fabs(y3-y2) &lt;= tolerance)
    {
        m1 = - (x2-x1) / (y2-y1);
        mx1 = (x1 + x2) / 2.0;
        my1 = (y1 + y2) / 2.0;
        *xc = (x3 + x2) / 2.0;
        *yc = m1 * (*xc - mx1) + my1;
    }
    else
    {
        m1 = - (x2-x1) / (y2-y1);
        m2 = - (x3-x2) / (y3-y2);
        mx1 = (x1 + x2) / 2.0;
        mx2 = (x2 + x3) / 2.0;
        my1 = (y1 + y2) / 2.0;
        my2 = (y2 + y3) / 2.0;
        *xc = (m1 * mx1 - m2 * mx2 + my2 - my1) / (m1 - m2);
        *yc = m1 * (*xc - mx1) + my1;
    }

    dx = x2 - *xc;
    dy = y2 - *yc;
    rsqr = dx*dx + dy*dy;
    *r = sqrt(rsqr);

    dx = xp - *xc;
    dy = yp - *yc;
    drsqr = dx*dx + dy*dy;

    return (drsqr &lt;= rsqr);
} /* CircumCircle */
