#pragma omp barrier
            // Update prod
#pragma omp for
            for (int64_t i=0;i&lt;int64_t(mA);i++)
            {
                for (uint j=0;j&lt;=iti-1;j++)
                {
                    x[i] += z[j][i] * y[j];
