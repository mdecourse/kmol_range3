<t tx="leo.20201108101531.107">void RLogger::flush (void)
{
    std::vector&lt;RMessage&gt;::iterator iter;

    RLocker::lock();
    for (iter = this-&gt;messages.begin();
         iter != this-&gt;messages.end();
         ++iter)
    {
