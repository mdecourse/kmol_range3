RMatrixPreconditioner::RMatrixPreconditioner(const RSparseMatrix &amp;matrix, RMatrixPreconditionerType matrixPreconditionerType, unsigned int blockSize)
    : matrixPreconditionerType(matrixPreconditionerType)
{
