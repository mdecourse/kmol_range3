double RR3Vector::angle(const RR3Vector &amp;v1, const RR3Vector &amp;v2)
{
    R_ERROR_ASSERT(v1.size() == v2.size());

    double ln = v1.length() * v2.length();
    if (ln == 0.0)
    {
        return 0.0;
    }

    double dotValue = RRVector::dot(v1,v2)/ln;

    if (dotValue &gt; 1.0)
    {
        dotValue = 1.0;
    }
    if (dotValue &lt; -1.0)
    {
        dotValue = -1.0;
    }

    return acos(dotValue);
} /* RR3Vector::angle */
