    bool ok;

    uint nIterations = item-&gt;text(column).toUInt(&amp;ok);

    if (ok &amp;&amp; nIterations &gt; 0)
    {
        item-&gt;setData(ProblemTaskTree::VALUE,Qt::UserRole,nIterations);
