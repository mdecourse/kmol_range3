    double convergenceRate = std::abs(1.0 - this-&gt;patchHeatNorm/this-&gt;oldPatchHeatNorm);

    RLogger::info("Convergence rate = %g\n",convergenceRate);

    return (convergenceRate &lt; RConstants::eps);
