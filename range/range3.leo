    PROBLEM_TREE_COLUMN_DESC,
    PROBLEM_TREE_N_COLUMNS
} ProblemSelectorTreeColumn;

ProblemSelectorTree::ProblemSelectorTree(QWidget *parent)
    : QTreeWidget(parent)
{
    this-&gt;setRootIsDecorated(false);
