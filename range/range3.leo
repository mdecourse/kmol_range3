    QDir dir(fileInfo.absolutePath());

    QStringList fileList = dir.entryList(nameFilters,QDir::Files,QDir::Reversed);

    QStringList fileNames;
    for (int i=0; i&lt;fileList.count();i++)
    {
        fileNames.append(dir.filePath(fileList.at(i)));
    }
    return fileNames;
}

