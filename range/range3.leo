{
    foreach (RSolverGeneric *solver, this-&gt;solvers)
    {
        delete solver;
    }
}

RSolver &amp;RSolver::operator =(const RSolver &amp;solver)
{
    this-&gt;_init(&amp;solver);
    return (*this);
}

