    this-&gt;setColumnHidden(MODEL_TREE_COLUMN_TYPE,true);
    this-&gt;setColumnHidden(MODEL_TREE_COLUMN_MID,true);
    this-&gt;setColumnHidden(MODEL_TREE_COLUMN_EID,true);

